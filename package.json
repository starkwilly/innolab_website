    "React",
    "Redux",
    "EsLint"
  ],
  "author": "Innolab Team<innolab.cr@accenture.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "clsx": "^1.1.1",
    "file-saver": "^2.0.5",
    "history": "^4.10.1",
    "immutability-helper": "^3.1.1",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "usetoaster": "^2.0.0",
    "xlsx": "^0.16.9"
  },
  "scripts": {
    "start": "node config && react-scripts start",
    "start:legacy": "node config --presets && react-scripts start",
    "prestart:dev": "npm install",
    "start:dev": "run-p start start:auth",
    "start:auth": "node _server/index.js",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.dev npm run build",
    "build:qa": "env-cmd -f .env.qa npm run build",
    "build:stage": "env-cmd -f .env.stage npm run build",
    "build:prod": "env-cmd -f .env.prod npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 11"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-preset-react-app": "^10.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "fs-extra": "^8.1.0",
    "json-server": "^0.15.0",
    "jsonwebtoken": "^8.5.1",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^6.1.0"
  }
}
